{
  "name": "react-reporter",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "nodemonConfig": {
    "events": {
      "restart": "clear"
    },
    "watch": [
      "server.js",
      "src/"
    ],
    "ext": "js"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",

    "axios": "^0.26.1",
    "bcryptjs": "^2.4.3",
    "chart.js": "^3.7.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^6.1.2",
    "morgan": "^1.10.0",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.29.0",
    "react-query": "^3.34.19",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^2.1.3",
    "serve-favicon": "^2.5.0",
    "web-vitals": "^2.1.4",

    "express-session": "^1.17.2",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "^1.4.5"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "express-start": "node server.js",
    "express-dev": "nodemon --inspect=0.0.0.0:9229 server/server.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}